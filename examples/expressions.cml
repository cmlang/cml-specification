@concept Expressions
{
    -- Literal Values:
    c: String = "SomeString";
    d: Integer = 123;

    -- Prefix Expression:
    prefixed_subtraction = -2;

    -- Infix Expressions:
    arithmetic = 1 + 2;
    relational = 3 == 3;
    logical = q and p;

    -- Conditional Expression:
    if_then_else = if a > 0 then a else b;

    -- Path Expression:
    path = somePath.bar;

    -- Comprehension Expression:
    selection = items | select: name == "this";
}
