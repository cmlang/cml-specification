// Empty concept:
concept EmptyBook;

// Attribute without a type:
concept TitledBook
{
    title;
}

// Attribute with the String type:
concept StringTitledBook
{
    title: String;
}

// Attribute with the Integer type:
concept QuantifiableBook
{
    quantity: Integer;
}

// Attribute with the Decimal type:
concept PricedBook
{
    price: Decimal;
}
