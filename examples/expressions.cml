concept Expressions
{
    -- Literal Values:
    c: String = "SomeString";
    d: Integer = 123;

    -- Prefix Expression:
    minus_sign = -2;

    -- Infix Expressions:
    addition = 1 + 2;
    equality = 3 == 3;
    boolean_expr = q and p;

    -- Conditional Expression:
    if_then_else = if a > 0 then a else b;

    -- Path Expression:
    path = somePath.bar;

    -- Query Expression:
    select_query = items | select name == "this";
}
