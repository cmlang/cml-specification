concept Expressions
{
    // Literals
    c: String = "SomeString";
    d: Integer = 123;

    // Unary Operators:
    plus_sign = +1;

    // Infix Operators:
    addition = 1 + 2;
    equality = 3 == 3;
    and_expr = q and p;

    // Path:
    path = somePath.bar;

    // Conditional:
    if_then_else = if a > 0 then a else b;

    // Query:
    select_transform = items | select name == "this";
}
